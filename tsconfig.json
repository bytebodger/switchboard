{
   "compilerOptions": {
      "allowJs": false,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "isolatedModules": true,
      "jsx": "react-jsx",
      "lib": [
         "dom",
         "dom.iterable",
         "ES2022"
      ],
      "module": "ES2022",
      "moduleResolution": "node",
      "noEmit": true,
      "noFallthroughCasesInSwitch": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "strict": true,
      "target": "ESNext"
   },
   "include": [
      "src"
   ]
}
